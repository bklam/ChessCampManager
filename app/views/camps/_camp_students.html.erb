<% unless @students.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5><%= "Registrations (#{@students.length})" %></h5>
    <ul class="side-nav">
      <% @students.each do |student| %>
        <li><%= link_to student.name, student_path(student) %> <%= "Rating: #{student.rating}" %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>No Registrations</h5>
  </div>
<% end %>