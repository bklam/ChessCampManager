<% unless @personal_upcoming_camps.empty? %>
  <!-- <h3>&nbsp;</h3> -->
    <h5>Your Upcoming Camps</h5>
      <% @personal_upcoming_camps.each do |camp| %>
        <li><%= link_to camp.name, camp_path(camp) %> (<%= camp.start_date.strftime('%m/%d/%y') %>)</li>
      <% end %>
<% else %>
    <h3>You have no upcoming camps... slacker</h3>
<% end %>