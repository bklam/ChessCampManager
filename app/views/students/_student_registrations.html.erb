<% unless @camps.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Camp Registrations <%= "(#{@camps.length})" %></h5>
    <ul class="side-nav">
      <% @camps.each do |camp| %>
        <li><%= link_to camp.name, camp_path(camp) %><%= "(#{camp.start_date.strftime("%m/%d/%y")})" %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>No Instructors</h5>
  </div>
<% end %>